#include<iostream>
using namespace std;
int main()
{
	int marks;
	cout<<"Enter the marks :";
	cin>>marks;
	
	if(marks<=100 && marks>85)
	{
		cout<<"Grade A";
	}
	
	else if(marks<=85 && marks>70)
	{
		cout<<"Grade B";
	}
	
	else if(marks<=70 && marks>55)
	{
		cout<<"Grade C";
	}
	
	else if(marks<=55 && marks>40)
	{
		cout<<"Grade D";
	}
	
	else if(marks<=40 && marks>25)
	{
		cout<<"Grade E";
	}
	
	else
	{
		cout<<"FAIL";
	}
	return 0;
}
